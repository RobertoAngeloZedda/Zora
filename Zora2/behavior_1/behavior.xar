                                                        <![CDATA[import os
import socket
import struct
import time

SERVER_ADDRESS = '192.168.29.97'
PORT = 12345
UPDATES_PER_SECOND = 20

class MyClass(GeneratedClass):
    def __init__(self):
        GeneratedClass.__init__(self)
        self.audio_queue = []

    def onLoad(self):
        #put initialization code here
        pass

    def onUnload(self):
        #put clean-up code here
        pass

    def connect(self, ip, port):
        client_socket = socket.socket(socket.AF_INET, socket.SOCK_STREAM)
        server_address = (ip, port)
        client_socket.connect(server_address)
        return client_socket

    def send_file(self, socket, file_name):
        # Send file size
        file_size = int(os.path.getsize(file_name))
        file_size = struct.pack('I', file_size)
        socket.sendall(file_size)
        # Send File data
        with open(file_name, 'rb') as file:
            data = file.read()
            socket.sendall(data)

    def receive_file(self, socket, file_name):
        # Receive file size
        file_size = socket.recv(4)
        file_size = struct.unpack('I', file_size)[0]
        # Receive file data
        with open(file_name, 'wb') as file:
            while file_size:
                data = socket.recv(min(1024, file_size))
                if not data:
                    print('Error while receiving file.')
                    break
                file.write(data)
                file_size -= len(data)

    def onInput_onStart(self):
        while True:
            if self.audio_queue:
                new_audio = self.audio_queue.pop(0)

                socket = self.connect(SERVER_ADDRESS, PORT)

                self.send_file(socket, new_audio)

                is_response_ready = socket.recv(1) == b'1'

                if is_response_ready:
                    #response_file = self.getParameter("response_file")
                    response_file = 'D:\\Documenti\\Universita\\Magistrale\\2A1S\\DeepLearningandApplications\\Zora2\\temp.txt'

                    self.receive_file(socket, response_file)

                    socket.close()

                    self.onStopped( response_file )
                    return
                else:
                    socket.close()

            else:
                time.sleep(1/UPDATES_PER_SECOND)

    def onInput_input(self, file_name):
        self.audio_queue.append(file_name)

    def onInput_onStop(self):
        self.onUnload() #it is recommended to reuse the clean-up as the box is stopped
        self.onStopped( self.response_file ) #activate the output of the box]]>
